<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
    <outbound-rule>
        <from>/product.html?id=$1</from>
        <to>/product/([0-9]+)</to>
    </outbound-rule>
</urlrewrite>